{
  "name": "cv-generator",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start:local": "env-cmd -e local -r ./.env-cmdrc.js vite",
    "start:dev": "env-cmd -e development -r ./.env-cmdrc.js vite",
    "start:build": "env-cmd -e production -r ./.env-cmdrc.js vue-tsc --noEmit && vite build",
    "start:preview": "vite preview",
    "start:lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src/",
    "format": "prettier . --write",
    "test": "env-cmd -e testing -r ./.env-cmdrc.js jest",
    "docs:dev": "env-cmd -e development -r ./.env-cmdrc.js vuepress dev src/docs",
    "docs:build": "env-cmd -e production -r ./.env-cmdrc.js vuepress build src/docs"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "env-cmd": "^10.1.0",
    "lodash": "^4.17.21",
    "vue": "^3.2.25",
    "vue-i18n": "9",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7"
  },
  "packageManager": "yarn@3.2.2"
}
